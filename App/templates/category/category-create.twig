{% extends 'base/baseTemplate.twig' %}
{% block content %}
    <div class="row">
        <div class="col-lg-12" id="careers">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 id="subjectName">Alta de Categoria</h4>
                </div>
                <div class="panel-body">
                    <form autocomplete="off" method="post" action="{{ path_for('category.create') }}">
                        <div class="form-group">
                            <label for="championshipName">Nombre</label>
                            <input type="text" name="categoryName" class="form-control" id="categoryName" placeholder="Categoria...">
                        </div>
                        <button type="submit" class="btn btn-default">Guardar</button>
                        {{ csrf.field | raw }}
                    </form>
                </div>
            </div>
        </div>
    </div>
{% endblock %}